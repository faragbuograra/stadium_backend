        "@types/express": "^4.17.13",
        "@types/jsdom": "^20.0.0",
        "@types/multer": "^1.4.7",
        "@types/node": "^18.0.0",
        "@types/pg": "^8.6.5",
        "@types/uuid": "^8.3.4",
        "axios": "^0.27.2",
        "jsdom": "^20.0.0",
        "nodemon": "^2.0.18",
        "ts-node": "^10.8.1",
        "typescript": "^4.7.4"
    },
