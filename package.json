        "express": "^4.18.1",
        "helmet": "^6.0.0",
        "jsonwebtoken": "^8.5.1",
        "knex": "^2.5.1",
        "moment": "^2.29.4",
        "ms": "^2.1.3",
        "multer": "^1.4.5-lts.1",
        "objection": "^3.1.2",
